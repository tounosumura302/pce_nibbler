  112                     HSR     .macro
  113                              .if (\1 <= 272)
  114                               ; low res
  115                               .db $02
  116                               .db (18 - (((\1 / 8) - 1) / 2))
  117                              .else
  118                               ; high res
  119                               .db $03
  120                               .db (24 - (((\1 / 8) - 1) / 2))
